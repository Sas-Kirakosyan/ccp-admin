{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "after-used",
        "varsIgnorePattern": "^_"
      }
    ],
    "prefer-const": [2, { "ignoreReadBeforeAssign": false }]
  }
}
